{
    "mnemonic": "interx.mnemonic",
    "cache": {
        "status_sync": 5,
        "cache_dir": "cache",
        "max_cache_size": "2 GB",
        "caching_duration": 10,
        "download_file_size_limitation": "10 MB"
    },
    "faucet": {
        "mnemonic": "faucet.mnemonic",
        "faucet_amounts": {
            "stake": 100000,
            "validatortoken": 100000,
            "ukex": 100000
        },
        "faucet_minimum_amounts": {
            "stake": 100,
            "validatortoken": 100,
            "ukex": 100
        },
        "fee_amounts": {
            "stake": "1000ukex",
            "validatortoken": "1000ukex",
            "ukex": "1000ukex"
        },
        "time_limit": 20
    },
    "rpc": {
        "API": {
            "GET": {
                "/api/cosmos/status": {
                    "rate_limit": 0.1,
                    "auth_rate_limit": 1,
                    "caching_duration": 30
                },
                "/api/cosmos/bank/supply": {
                    "disable": true,
                    "auth_rate_limit": 1,
                    "caching_disable": true,
                    "caching_duration": 30
                }
            },
            "POST": {
                "/api/cosmos/txs": {
                    "disable": false,
                    "rate_limit": 0.1
                }
            }
        }
    }
}